#ifndef Singly_Linked_List_H_
#define Singly_Linked_List_H_

class SinglyLinkedList {
public:
    SinglyLinkedList();
    int length(){ return nodes; }
    void append(int data);
private:
    struct Node {
        Node *next;
        int   data;
    };
    Node *first;
    Node *last;
    int nodes;
};

#endif
